{
  "name": "mern-trendflow-ai",
  "version": "1.0.2",
  "description": "[TrendFlow](https://trendflowai.com)",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "setup-project": "npm install && cd client && npm install",
    "server": "nodemon --legacy-watch server.js",
    "start-server": "NODE_ENV=production node server.js",
    "client": "cd client && npm run dev",
    "dev": "NODE_ENV=development concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "build": "npm run client",
    "build:emails": "babel emails --out-dir dist/emails --extensions \".js,.jsx\"",
    "start": "npm run build && npm run start-server",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-email/components": "^0.0.33",
    "@react-email/render": "^1.0.5",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.37.3",
    "compression": "^1.7.5",
    "concurrently": "^8.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "datauri": "^4.1.0",
    "dayjs": "^1.11.9",
    "dompurify": "^3.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.6.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "nodemon": "^2.0.22",
    "openai": "^3.3.0",
    "react-email": "^3.0.7",
    "redis": "^4.6.13",
    "sharp": "^0.32.6",
    "shell-quote": "^1.8.1",
    "sitemap": "^8.0.0",
    "validator": "^13.12.0"
  },
  "engines": {
    "node": ">=19.3.0",
    "npm": ">=9.2.0"
  },
